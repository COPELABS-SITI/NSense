<?xml version="1.0" encoding="UTF-8"?>
<defaultsettings>
<sensors>
		<sensor>
		    	<sensorname>GPS</sensorname>
		    	<sensorinterval>1000</sensorinterval>
		    	<classname>LocationManager.GPS</classname>
